<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance">
        <item name="android:textColor">#ff33b5e5</item>
        <item name="android:textColorHighlight">#ff33b5e5</item>
        <item name="android:textColorHint">#ff33b5e5</item>
        <item name="android:textColorLink">#ff33b5e5</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="TextAppearance.SearchResult">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ff33b5e5</item>
        <item name="android:textColorHint">#ff33b5e5</item>
    </style>

    <style name="TextAppearance.SearchResult.Title">
        <item name="android:textSize">16sp</item>
    </style>

    <style name="TextAppearance.SearchResult.Subtitle">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#ff33b5e5</item>
    </style>

    <style name="MmsHoloTheme" parent="@android:style/Theme.Holo">
    </style>

    <style name="WidgetTitle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#ff33b5e5</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="NotificationPrimaryText">
        <item name="android:textColor">#ff33b5e5</item>
    </style>

    <style name="NotificationSecondaryText">
        <item name="android:textColor">#ff33b5e5</item>
    </style>

    <style name="NotificationSubjectText">
        <item name="android:textColor">#ff33b5e5</item>
    </style>

</resources>
